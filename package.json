{
  "name": "quick-snippet-inserter",
  "displayName": "Quick Snippet Inserter",
  "description": "🚀 Extensión profesional para insertar snippets de código con plantillas predefinidas y personalizadas. Interfaz mejorada, 20+ snippets incluidos, y gestión completa de plantillas.",
  "version": "0.1.0",
  "publisher": "quick-snippet-team",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets",
    "Other",
    "Productivity",
    "Keymaps"
  ],
  "keywords": [
    "snippets",
    "templates",
    "productivity",
    "code-generation",
    "javascript",
    "typescript",
    "html",
    "css",
    "python",
    "boilerplate",
    "quick-insert",
    "code-templates",
    "custom-snippets",
    "snippet-manager"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "icon": "icon.png",
  "qna": "marketplace",
  "sponsor": {
    "url": "https://github.com/sponsors/quick-snippet-team"
  },
  "activationEvents": [
    "onCommand:quickSnippet.insertSnippet",
    "onCommand:quickSnippet.listSnippets", 
    "onCommand:quickSnippet.insertConsoleLog",
    "onCommand:quickSnippet.searchSnippets",
    "onCommand:quickSnippet.insertByCategory",
    "onCommand:quickSnippet.createTemplate",
    "onCommand:quickSnippet.editTemplate",
    "onCommand:quickSnippet.deleteTemplate",
    "onCommand:quickSnippet.importTemplates",
    "onCommand:quickSnippet.exportTemplates",
    "onCommand:quickSnippet.openTemplatesFile",
    "onCommand:quickSnippet.reloadTemplates",
    "onCommand:quickSnippet.templateStats",
    "onCommand:quickSnippet.showMainMenu",
    "onCommand:quickSnippet.manageTemplates",
    "onCommand:quickSnippet.importExportMenu",
    "onCommand:quickSnippet.exploreByLanguage",
    "onCommand:quickSnippet.quickContextSnippet",
    "onCommand:quickSnippet.openSettings"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quickSnippet.insertSnippet",
        "title": "Insertar Snippet Rápido",
        "category": "Quick Snippet",
        "icon": "$(symbol-snippet)"
      },
      {
        "command": "quickSnippet.listSnippets",
        "title": "Ver Lista de Snippets",
        "category": "Quick Snippet",
        "icon": "$(list-unordered)"
      },
      {
        "command": "quickSnippet.insertConsoleLog",
        "title": "Insertar console.log",
        "category": "Quick Snippet",
        "icon": "$(debug-console)"
      },
      {
        "command": "quickSnippet.searchSnippets",
        "title": "Buscar Snippets",
        "category": "Quick Snippet",
        "icon": "$(search)"
      },
      {
        "command": "quickSnippet.insertByCategory",
        "title": "Insertar por Categoría",
        "category": "Quick Snippet",
        "icon": "$(folder)"
      },
      {
        "command": "quickSnippet.createTemplate",
        "title": "Crear Snippet Personalizado",
        "category": "Quick Snippet",
        "icon": "$(add)"
      },
      {
        "command": "quickSnippet.editTemplate",
        "title": "Editar Snippet Personalizado",
        "category": "Quick Snippet",
        "icon": "$(edit)"
      },
      {
        "command": "quickSnippet.deleteTemplate",
        "title": "Eliminar Snippet Personalizado",
        "category": "Quick Snippet",
        "icon": "$(trash)"
      },
      {
        "command": "quickSnippet.importTemplates",
        "title": "Importar Snippets desde Archivo",
        "category": "Quick Snippet",
        "icon": "$(cloud-download)"
      },
      {
        "command": "quickSnippet.exportTemplates",
        "title": "Exportar Snippets a Archivo",
        "category": "Quick Snippet",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "quickSnippet.openTemplatesFile",
        "title": "Abrir Archivo de Snippets",
        "category": "Quick Snippet",
        "icon": "$(file-code)"
      },
      {
        "command": "quickSnippet.reloadTemplates",
        "title": "Recargar Snippets Personalizados",
        "category": "Quick Snippet",
        "icon": "$(refresh)"
      },
      {
        "command": "quickSnippet.templateStats",
        "title": "Ver Estadísticas de Snippets",
        "category": "Quick Snippet",
        "icon": "$(graph)"
      },
      {
        "command": "quickSnippet.showMainMenu",
        "title": "Menú Principal",
        "category": "Quick Snippet",
        "icon": "$(menu)"
      },
      {
        "command": "quickSnippet.manageTemplates",
        "title": "Gestionar Snippets Personalizados",
        "category": "Quick Snippet",
        "icon": "$(tools)"
      },
      {
        "command": "quickSnippet.importExportMenu",
        "title": "Menú Importar/Exportar",
        "category": "Quick Snippet",
        "icon": "$(package)"
      },
      {
        "command": "quickSnippet.exploreByLanguage",
        "title": "Explorar por Lenguaje",
        "category": "Quick Snippet",
        "icon": "$(code)"
      },
      {
        "command": "quickSnippet.quickContextSnippet",
        "title": "Snippets Rápidos del Contexto",
        "category": "Quick Snippet",
        "icon": "$(zap)"
      },
      {
        "command": "quickSnippet.openSettings",
        "title": "Abrir Configuración",
        "category": "Quick Snippet",
        "icon": "$(settings)"
      }
    ],
    "keybindings": [
      {
        "command": "quickSnippet.insertSnippet",
        "key": "ctrl+shift+s",
        "mac": "cmd+shift+s",
        "when": "editorTextFocus"
      },
      {
        "command": "quickSnippet.insertConsoleLog",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      },
      {
        "command": "quickSnippet.listSnippets",
        "key": "ctrl+shift+alt+s",
        "mac": "cmd+shift+alt+s"
      },
      {
        "command": "quickSnippet.searchSnippets",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f",
        "when": "editorTextFocus"
      },
      {
        "command": "quickSnippet.insertByCategory",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c",
        "when": "editorTextFocus"
      },
      {
        "command": "quickSnippet.createTemplate",
        "key": "ctrl+shift+n",
        "mac": "cmd+shift+n",
        "when": "editorTextFocus"
      },
      {
        "command": "quickSnippet.templateStats",
        "key": "ctrl+shift+alt+t",
        "mac": "cmd+shift+alt+t"
      },
      {
        "command": "quickSnippet.showMainMenu",
        "key": "ctrl+shift+m",
        "mac": "cmd+shift+m"
      },
      {
        "command": "quickSnippet.exploreByLanguage",
        "key": "ctrl+shift+alt+l",
        "mac": "cmd+shift+alt+l"
      },
      {
        "command": "quickSnippet.quickContextSnippet",
        "key": "ctrl+shift+q",
        "mac": "cmd+shift+q",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "quickSnippet.insertSnippet",
          "when": "editorTextFocus",
          "group": "1_modification"
        },
        {
          "command": "quickSnippet.insertConsoleLog",
          "when": "editorTextFocus && resourceExtname =~ /\\.(js|ts|jsx|tsx)$/",
          "group": "1_modification"
        },
        {
          "command": "quickSnippet.searchSnippets",
          "when": "editorTextFocus",
          "group": "1_modification"
        },
        {
          "command": "quickSnippet.insertByCategory",
          "when": "editorTextFocus",
          "group": "1_modification"
        }
      ],
      "commandPalette": [
        {
          "command": "quickSnippet.insertSnippet"
        },
        {
          "command": "quickSnippet.listSnippets"
        },
        {
          "command": "quickSnippet.insertConsoleLog"
        },
        {
          "command": "quickSnippet.searchSnippets"
        },
        {
          "command": "quickSnippet.insertByCategory"
        },
        {
          "command": "quickSnippet.createTemplate"
        },
        {
          "command": "quickSnippet.editTemplate"
        },
        {
          "command": "quickSnippet.deleteTemplate"
        },
        {
          "command": "quickSnippet.importTemplates"
        },
        {
          "command": "quickSnippet.exportTemplates"
        },
        {
          "command": "quickSnippet.openTemplatesFile"
        },
        {
          "command": "quickSnippet.reloadTemplates"
        },
        {
          "command": "quickSnippet.templateStats"
        },
        {
          "command": "quickSnippet.showMainMenu"
        },
        {
          "command": "quickSnippet.manageTemplates"
        },
        {
          "command": "quickSnippet.importExportMenu"
        },
        {
          "command": "quickSnippet.exploreByLanguage"
        },
        {
          "command": "quickSnippet.quickContextSnippet"
        },
        {
          "command": "quickSnippet.openSettings"
        }
      ]
    },
    "configuration": {
      "title": "Quick Snippet Inserter",
      "properties": {
        "quickSnippet.enableWelcomeMessage": {
          "type": "boolean",
          "default": true,
          "description": "Mostrar mensaje de bienvenida al activar la extensión"
        },
        "quickSnippet.defaultLanguage": {
          "type": "string",
          "default": "javascript",
          "description": "Lenguaje por defecto para snippets",
          "enum": ["javascript", "typescript", "python", "java", "csharp", "other"]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish",
    "lint": "eslint src --ext .ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "clean": "rimraf out",
    "build": "npm run clean && npm run compile"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "vsce": "^2.15.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/quick-snippet-team/quick-snippet-inserter.git"
  },
  "bugs": {
    "url": "https://github.com/quick-snippet-team/quick-snippet-inserter/issues"
  },
  "homepage": "https://github.com/quick-snippet-team/quick-snippet-inserter#readme",
  "license": "MIT"
} 